<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sba.deathrace">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iconsmall"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sba.deathrace.FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sba.deathrace.GameOverActivity" />
        <activity android:name="com.sba.deathrace.MainActivity">
        </activity>
    </application>

</manifest>